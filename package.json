{
  "name": "genenotebook",
  "version": "0.2.0",
  "repository": "https://github.com/genenotebook/genenotebook",
  "description": "A portable website for browsing and querying genome sequences and annotations",
  "license": "AGPL-3.0",
  "scripts": {
    "preinstall": "meteor help || curl https://install.meteor.com/ | sh",
    "preupdate": "meteor update",
    "docs": "documentation build --shallow imports/** -f html -o api-docs",
    "postdocs": "cp api-docs/index.html docs/pages/api-docs.html && cp -r api-docs/assets docs/",
    "bundle": "./bundle.sh",
    "complete_build": "npm install && npm run bundle",
    "dev": "NODE_OPTIONS=--max-old-space-size=8192 meteor run --settings settings.json",
    "dev_production": "NODE_OPTIONS=--max-old-space-size=8192 meteor run --production --settings settings.json",
    "reset": "meteor reset",
    "lint": "eslint imports",
    "postversion": "pushd scripts && npm version $(jq -r .version ../package.json) && popd",
    "test": "TEST_CLIENT=0 TEST_BROWSER_DRIVER=puppeteer BABEL_ENV=COVERAGE COVERAGE=1 COVERAGE_OUT_HTML=1 COVERAGE_APP_FOLDER=$PWD/ meteor --verbose test --once --driver-package meteortesting:mocha --raw-logs"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  },
  "babel": {
    "env": {
      "COVERAGE": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "@meteorjs/eslint-config-meteor",
    "settings": {
      "import/resolver": {
        "meteor": {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "asteroid": "^2.0.3",
    "bcrypt": "^5.0.0",
    "bootstrap": "^4.4.1",
    "buffer-shims": "^1.0.0",
    "bufferutil": "^4.0.1",
    "child-process": "^1.0.2",
    "color": "^3.1.2",
    "commander": "^7.0.0",
    "core-js": "^3.6.4",
    "core-util-is": "^1.0.2",
    "d3": "^6.0.0",
    "documentation": "^13.0.0",
    "dot-object": "^2.1.2",
    "fibers": "^5.0.0",
    "file-type": "^16.0.0",
    "fs.extra": "^1.3.2",
    "glob": "^7.1.6",
    "html-entities": "^2.0.0",
    "immutability-helper": "^3.0.1",
    "install": "^0.13.0",
    "lodash": "^4.17.15",
    "meteor-babel-helpers": "0.0.3",
    "meteor-job": "^1.5.2",
    "meteor-later": "^1.2.0",
    "meteor-node-stubs": "^1.0.0",
    "object-hash": "^2.0.1",
    "papaparse": "^5.2.0",
    "pkginfo": "^0.4.1",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.6.0",
    "react": "^17.0.0",
    "react-container-dimensions": "^1.4.1",
    "react-dom": "^17.0.0",
    "react-mounter": "^1.2.0",
    "react-resize-detector": "^6.0.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^4.0.0",
    "reactstrap": "^8.4.0",
    "readline-promise": "^1.0.4",
    "recompose": "^0.30.0",
    "rus-diff": "^1.1.0",
    "simpl-schema": "^1.5.6",
    "spawn-promise": "^0.1.8",
    "spawn-sync": "^2.0.0",
    "tail": "^2.0.3",
    "thread-sleep": "^2.2.0",
    "try-thread-sleep": "^2.0.0",
    "utf-8-validate": "^5.0.2",
    "webfontloader": "^1.6.28",
    "ws": "^7.2.1",
    "xml2js-es6-promise": "^1.1.1"
  },
  "devDependencies": {
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "babel-eslint": "^10.0.3",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-meteor": "^7.0.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "puppeteer": "^8.0.0"
  }
}
